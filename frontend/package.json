{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "targets": {
    "admin": {
      "source": "src/admin/index/templates/index.html",
      "distDir": "dist/admin/index",
      "publicUrl": "/static/admin/index"
    },
    "website.common": {
      "source": "src/website/common/templates/base.hbs",
      "distDir": "dist/website/common",
      "publicUrl": "/static/website/common"
    },
    "website.index": {
      "source": "src/website/index/templates/index.hbs",
      "distDir": "dist/website/index",
      "publicUrl": "/static/website/index"
    }
  },
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "rules": {
      "(.*)\\.(hbs)": "templates/$1.hbs",
      "(.*)\\.(html)": "templates/$1.html"
    }
  },
  "browserlist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@parcel/config-default": "2.5.0",
    "@parcel/packager-html": "2.5.0",
    "@parcel/packager-raw-url": "2.5.0",
    "@parcel/transformer-html": "2.5.0",
    "@parcel/transformer-posthtml": "2.5.0",
    "@parcel/transformer-webmanifest": "2.5.0",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "livereload": "0.9.3",
    "parcel": "2.5.0",
    "parcel-namer-rewrite": "2.0.0-rc.2",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint . --ext .ts,.tsx --fix",
    "**/*": "prettier --write --ignore-unknown",
    "../backend/**/*.rs": "rustfmt --edition 2021"
  },
  "scripts": {
    "start:admin": "parcel src/admin/index/templates/index.html",
    "build": "parcel build",
    "build:dev": "parcel build",
    "start:website": "yarn livereload & ./start.sh",
    "postinstall": "cd .. && husky install frontend/.husky && cd frontend && ./build.sh",
    "livereload": "livereload -e 'hbs,js,css' ../backend/static"
  }
}
